
    <h1>What would you like to cook today?</h1>

    <form class="container">
      <input type="search" name="food" value="">
      <button type="search" name="button">Search</button>
    </form>

    <% unless @video.nil? %>
    <ul>
      <% @video.each do |key, value| %>
      <li><iframe class="youtube" width="560" height="315" src="https://www.youtube.com/embed/<%= key["id"]["videoId"] %>" frameborder="0" allowfullscreen></iframe></li>

      <% end %>
    </ul>

    <% end %>
